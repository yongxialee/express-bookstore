{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "isbn",
      "author",
      "language",
      "pages",
      "title",
      "year"
    ],
    "properties": {
        "isbn": {
            "$id": "#/properties/isbn-10",
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "examples": [
                "0691161518"
            ]
        },
        "amazon_url": {
            "$id": "#/properties/amazon-url",
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": [
                "http://a.co/eobPtX2"
            ]
        },
        "author": {
            "$id": "#/properties/author",
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "Matthew Lane"
            ]
        },
        "language": {
            "$id": "#/properties/language",
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "english"
            ]
        },
        "pages": {
            "$id": "#/properties/pages",
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                264
            ]
        },
        "publisher": {
            "$id": "#/properties/publisher",
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": [
                "Princeton University Press"
            ]
        },
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Power-Up: Unlocking the Hidden Mathematics in Video Games"
            ]
        },
        "year": {
            "$id": "#/properties/year",
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                2017
            ]
        }
    }
}